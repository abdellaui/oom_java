public class QuaderTest {
	public static void main(String[] args){
		Quader q1 = new Quader();
		q1.laenge = 10.0;
		q1.breite = 5.0;
		q1.hoehe = 4.0;
		System.out.println("Volumen:" + q1.berechneVolumen() + "m^3");
		
		Quader q2 = new Quader();
		q2.laenge = 5.0;
		q2.breite = 5.0;
		q2.hoehe = 5.0;
		System.out.println("Volumen:" + q2.berechneVolumen() + "m^3");
		
	}
}
